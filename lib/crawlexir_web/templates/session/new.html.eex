<h1>Sign in</h1>

<%= form_for @conn, Routes.session_path(@conn, :create), fn f -> %>
  <%= label f, :email %>
  <%= text_input f, :email %>

  <%= label f, :password %>
  <%= text_input f, :password %>

  <div>
    <%= submit "Sign in" %>
  </div>

    <p>Don't have an account? <%= link "Sign up", to: Routes.registration_path(@conn, :new) %> for a new account.</p>
<% end %>

<%= form_for @conn, Routes.upload_path(@conn, :create), [multipart: true, as: :upload], fn f -> %>
  <div class="form-group">
    <%= label f, :csv_file %>
    <%= file_input f, :csv_file  %>
  </div>

  <div class="form-group">
    <%= submit "Upload", class: "btn btn-primary" %>
  </div>
<% end %>
